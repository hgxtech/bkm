<%- model_class = Plans -%>
<div class="page-header">
  <h1><%=t '.title', :default => Plans %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:notes) %></dt>
      <dd><pre class="prettyprint"><%= @plans.notes %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:from_date) %></dt>
      <dd><pre class="prettyprint"><%= @plans.from_date %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:to_date) %></dt>
      <dd><pre class="prettyprint"><%= @plans.to_date %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              plans_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_plans_path(@plans), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              plans_path(@plans),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

